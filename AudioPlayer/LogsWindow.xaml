<Window x:Class="AudioPlayer.LogsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AudioPlayer"
        mc:Ignorable="d"
        Title="LogsWindow" Height="400" Width="450" Closing="Window_Closing">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label Content="История прослушивания" FontFamily="Cascadia Code SemiBold" HorizontalAlignment="Center"/>
        <ListBox x:Name="logsListBox" FontFamily="Cascadia Code SemiBold" Grid.Row="1"/>
    </Grid>
</Window>
